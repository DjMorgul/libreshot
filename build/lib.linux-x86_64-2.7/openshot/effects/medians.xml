<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE openshot-effect>
<effect>
	<title translatable="True">Denoise via median</title>
	<description translatable="True">Median filter to e.g. remove salt and pepper noise</description>
	<icon>medians.png</icon>
	<category>Video</category>
	<service>frei0r.medians</service>
	
	<param name="type" type="spinner" title="Type" description="1: Cross; 2: 3x3 Square; 3: Bilevel; ..."> 
		<min>1</min> 
		<max>11</max>
		<step>1</step> 
		<default>1</default> 
	</param> 
 
	<param name="size" type="spinner" title="Size" description=""> 
		<min>1</min> 
		<max>20</max>
		<step>1</step>
		<default>1</default> 
	</param> 

</effect>
